@import '../../variables'

.tag
    box-sizing border-box
    padding-top 8px
    padding-bottom 8px
    padding-left 15px
    padding-right 15px
    border-radius 23px
    font-size 13px
    background-color $darkenBackground
    &:hover
        cursor pointer
    &--active
        background-color $YandexYellow